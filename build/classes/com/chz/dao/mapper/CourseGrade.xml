<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chz.dao.courseGrade">
   <!--  根据id 查找第一级别 -->
   <select id="getCourseGradeById"   parameterType="Integer"     resultType="CourseGrade">
		  select   *   from   td_course_grade1   where  
		  id=#{firstid} 
	</select>
</mapper>